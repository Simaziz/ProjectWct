{
  
 
  
  
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,


    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".", // Base directory for module resolution
    "paths": {
      "@/*": ["./src/*"] // Map @/ to the src folder
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "lib/models/User.js", "src/app/api/users/route.js", "src/models/User.ts", "src/app/api/pfTrainer/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
